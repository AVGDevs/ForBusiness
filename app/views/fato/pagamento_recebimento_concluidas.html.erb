<h1>Tarefas Conclu√≠das de Pagamento/Recebemento</h1>

<table class="table table-striped table-hover">
    <tr>
        <th>Tarefa</th>
        <th>Empresa</th>
        <th>Data de vencimento</th>
    </tr>

<% @pagamento_recebimento_concluidas.each do |f| %>
    <tr>
        <td> <%= f.tarefa.nomeTarefa %> </td>
        <td> <%= f.empresa.nomeEmpresa %> </td>
        <td> <%= f.dataVencimentoTarefa %> </td>
        <td> <%= link_to image_tag("uncheck.png"), action: "desfazer_pagamento_recebimento", id: f %> </td>
    </tr>
<% end %>
</table>
