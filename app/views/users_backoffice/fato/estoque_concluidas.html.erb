<h1>Tarefas Conclu√≠das do Estoque</h1>

<table class="table table-striped table-hover">
    <tr>
        <th>Tarefas do Estoque</th>
        <th>Empresa</th>
        <th>Data de vencimento</th>
    </tr>

<% @estoque_concluidas.each do |f| %>
    <tr>
        <td> <%= f.tarefa.nomeTarefa %> </td>
        <td> <%= f.empresa.nomeEmpresa %> </td>
        <td> <%= f.dataVencimentoTarefa %> </td>
        <td> <%= link_to image_tag("uncheck.png"), action: "desfazer_estoque", id: f %> </td>
    </tr>
<% end %>
</table>
